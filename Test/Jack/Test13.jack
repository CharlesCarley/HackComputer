	function int get(int address) {
		return ram[address];
	}

	function int set(int address, int value) {
		let ram[address] = value;
		return ram[address];
	}

	function void main() {

		var int j, b, t, d;

		do Main.init();

		let b = 0;


		while ( 1 = 1 ) {
			let j = 0;
			while (j < 8192)
			{
				let t = Main.get(j);
				let d = Main.set(j, t+2);
				let j = j+1;
			}
			let b = b+1;
		}
		return Main.get(0);
	}
}
